<h1>Padrón de aspirantes</h1>


<div class="container-fluid">
  <div class="row post_row">
    <div class="col-md-8">



      <table class="table table-striped">
        <tr>
          <th>Id</th>
          <th>Nombre</th>
          <th>Apellido Paterno</th>
          <th>Apellido Materno</th>
          <th>Nombre</th>
          <th>Fecha de nacimiento</th>
          <th>CURP</th>
          <th>Estado</th>
          <th>Localidad</th>
          <th>Lengua</th>
          <th>Descripción</th>


        </tr>

        <% @objeto_pg1.each_row do |item| %>
            <tr>
              <td><%= item[0] %></td>
              <td><%= item[1] %></td>
              <td><%= item[3] %></td>
              <td><%= item[4] %></td>
              <td><%= item[5] %></td>
              <td><%= item[6] %></td>
              <td><%= item[7] %></td>
              <td><%= item[8] %></td>
              <td><%= item[9] %></td>

            </tr>
        <% end %>
      </table>

      <br />



    </div>
  </div>
</div>

