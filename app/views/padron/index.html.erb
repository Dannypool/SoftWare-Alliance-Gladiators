<div class="container-fluid">
  <div class="row post_row">
    <div class="col-md-8">



      <table class="table table-striped">
        <tr>
          <th>Nombre</th>
          <th>Apellido Paterno</th>
          <th>Apellido Materno</th>
          <th>Fecha de nacimiento</th>
          <th>Sexo</th>
          <th>Estado</th>
          <th>Municipio</th>
          <th>Localidad</th>
          <th>Cantidad</th>
          <th>Sede</th>
          <th>Lengua indigena</th>

        </tr>

        <% @padron.each do |item| %>
            <tr>
              <td><%= item.id %></td>
              <td><%= item.nombre %></td>
              <td><%= item.a_paterno %></td>
              <td><%= item.a_materno %></td>
              <td><%= item.fecha_nac %></td>
              <td><%= item.price_your %></td>
              <td><%= item.sexo %></td>
              <td><%= item.sexo %></td>


              <td><%= item.statistics.select(:item_url).map(&:item_url).join("") %></td>
            </tr>
        <% end %>
      </table>

      <br />

      <%= link_to 'Nuevo item', new_item_path %>

    </div>
  </div>
</div>

